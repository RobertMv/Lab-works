import math
r1 = int(input("Введите радиус первой окружности = "))
r2 = int(input("Введите радиус второй окружности = "))

s1 = math.pi*r1**2
s2 = math.pi*r2**2

if s2>s1: s3 = s2 - s1
if s2<s1: s3 = s1 - s2
if s2==s1: print ("Площадь кольца = 0")


print ("Площадь первого круга = ", s1)
print ("Площадь второго круга = ", s2)
print ("Площадь кольца = ", s3)
